{
  "name": "node-storage-manager",
  "version": "1.0.0",
  "description": "Asyncronous Node storage manager in type script",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean": "rimraf tsconfig.tsbuildinfo ./dist",
    "cleanBuild": "npm run clean && tsc",
    "deepClean": "rimraf tsconfig.tsbuildinfo package-lock.json ./dist ./node_modules",
    "prepublishOnly": "npm run lint && npm run cleanBuild",
    "checkDependencies": "npx npm-check-updates",
    "updateDependencies": "npx npm-check-updates -u"
  },
  "keywords": [
    "node-persist"
  ],
  "author": "https://github.com/Luligu",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/node-persist": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-persist": "^4.0.1"
  }
}
